{
  "name": "Uniform Cookie UI", 
  "service_worker_scripts": [
    "js/worker.js"
  ], 
  "content_scripts": [
    "js/content.js"
  ], 
  "content_include": [
    "js/rules.json",
    "images/cookie-green.png",
    "images/cookie-idle.png",
    "images/cookie-red.png",
    "images/cookie-yellow.png"
  ], 
  "permissions": [
    "cookies",
    "tabs",
    "scripting",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
